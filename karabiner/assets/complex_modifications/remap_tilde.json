{
  "title": "british custom",
  "rules": [
    {
      "description": "remap tilde key shift + backslash",
        "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "backslash",
              "modifiers": {
              "mandatory": [
                "shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "grave_accent_and_tilde",
              "modifiers": [
                "shift"
            ]
            }
          ]
        }
      ]
    }
  ]
}
