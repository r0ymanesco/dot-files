{
  "title": "british custom",
  "rules": [
    {
      "description": "remap grave accent to non-US backslash (pm)",
        "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "non_us_backslash"
          },
          "to": [
            {
              "key_code": "grave_accent_and_tilde"
            }
          ]
        }
      ]
    }
  ]
}
